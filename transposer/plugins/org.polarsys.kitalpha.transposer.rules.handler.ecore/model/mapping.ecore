      <eStructuralFeatures xsi:type="ecore:EAttribute" name="ownedDomainHelper" eType="#//api/IDomainHelper"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="domainHelper" lowerBound="1"
