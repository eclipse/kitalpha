<?eclipse version="3.0"?>
<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.polarsys.kitalpha.ad.viewpoint.dsl.as.model.vpdesc.presentation.VpdescModelWizardID"
            name="%_UI_VpdescModelWizard_label"
            class="org.polarsys.kitalpha.ad.viewpoint.dsl.as.model.vpdesc.presentation.VpdescModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/VpdescModelFile.gif">
         <description>%_UI_VpdescModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.polarsys.kitalpha.ad.viewpoint.dsl.as.model.vpdesc.presentation.VpdescEditorID"
            name="%_UI_VpdescEditor_label"
            icon="icons/full/obj16/VpdescModelFile.gif"
            extensions="vpdesc"
            class="org.polarsys.kitalpha.ad.viewpoint.dsl.as.model.vpdesc.presentation.VpdescEditor"
            contributorClass="org.polarsys.kitalpha.ad.viewpoint.dsl.as.model.vpdesc.presentation.VpdescActionBarContributor">
      </editor>
   </extension>

</plugin>
